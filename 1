void main() {
  String nome = 'Luiz';
  int idade = 17;
  
  if (idade <= 1 && idade <= 13){
    print('O ${nome}  é uma criança');
  } else if (idade > 13 && idade < 18) {   
    print('O ${nome} é um Adolecente');
  } else if (idade == 18 && idade <= 59) {   
    print('O ${nome} é um adulto');
  } else {   
    print('O ${nome} é um Veio');
  }
}
